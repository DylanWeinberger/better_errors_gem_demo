						<%= form_for @profile do |f| %>
						<%= f.label :first_name %>
						<%= f.text_field :first_name %>
						<br>
						<br>
						<%= f.label :last_name %>
						<%= f.text_field :last_name %>
						<br>
						<br>
						<%= f.label :city %>
						<%= f.text_field :city %>
						<br>
						<br>
						<%= f.label :age %>
						<%= f.number_field :age %>
						<br>
						<br>
						<%= f.submit :create %>
						<% end %>